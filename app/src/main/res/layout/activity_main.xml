<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".entity.items" android:id="@+id/frameLayout5">

    <!-- TODO: Update blank fragment layout -->
    <fragment
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:name="com.example.mybmi.fragment.HeaderFragment"
            android:id="@+id/headerFragmet"
            tools:layout="@layout/fragment_header"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
    />
    <fragment
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:name="com.example.mybmi.fragment.FooterFragment"
            android:id="@+id/footerFragment"
            tools:layout="@layout/fragment_footer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
    />
    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/container" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/headerFragmet" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/footerFragment" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>